!urxvt extensions, remove select to clipboard:
URxvt.perl-ext-common:  default,matcher,clipboard,resize-font,-selection-to-clipboard

!enabling clickable links:
!URxvt.url-launcher:      /usr/bin/firefox
URxvt.url-launcher:      /usr/bin/google-chrome
URxvt.matcher.button:   1

!urxvt scrolling options and cursor style:
URxvt.saveLines: 12000
URxvt.scrollBar: false
!URxvt.cursorBlink: true
!URxvt.cursorUnderline: 

URxvt.iso14755: false
URxvt.iso14755_52: false

!clipboard shortcuts
URxvt.keysym.Control-Shift-C:   perl:clipboard:copy
URxvt.keysym.Control-Shift-V:   perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped

!clipboard commands
URxvt.clipboard.copycmd:  xclip -i -selection clipboard
URxvt.clipboard.pastecmd: xclip -o -selection clipboard
